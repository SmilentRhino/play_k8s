apiVersion: apps/v1beta1 
kind: Deployment
metadata:
  name: blackbox-exporter 
  namespace: {{ data.namespace }} 
spec:
  replicas: "1"
  template:
    metadata:
      labels:
        task: monitoring
        name: blackbox-exporter
    spec:
      containers:
      - name: blackbox-exporter
        image: prom/blackbox-exporter 
